{
   "background": {
      "scripts": [ "js/jquery.js", "js/background.js" ]
   },
   "browser_action": {
      "default_icon": "img/tao48.png",
       "default_popup": "html/popup.html",
       "default_title": "手机详情页生成"
   },
   "content_scripts": [ {
       "js": [ "js/jquery.js", "js/content.js" ],
       "matches": [ "http://upload.taobao.com/*" ]
   }],
   "description": "手机详情页生成",
   "icons": {
      "128": "img/tao128.png",
      "16": "img/tao16.png",
      "48": "img/tao48.png"
   },
   "manifest_version": 2,
   "name": "手机详情页生成",
   "permissions": ["http://*.tobti.com/", "http://fenxiao.youmiguang.com/", "http://*.taobao.com/", "tabs", "cookies", "notifications" ],
   "version": "0.2",
   "web_accessible_resources": [ "img/tao48.png" ]
}
